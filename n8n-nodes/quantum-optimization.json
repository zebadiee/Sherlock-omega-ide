{
  "name": "Quantum Build Optimizer",
  "version": "1.0.0",
  "description": "Quantum-enhanced build optimization for development workflows",
  "displayName": "Quantum Optimizer",
  "icon": "file:quantum.svg",
  "group": ["development", "optimization"],
  "inputs": ["main"],
  "outputs": ["main"],
  "credentials": [],
  "properties": [
    {
      "displayName": "Build Type",
      "name": "buildType",
      "type": "options",
      "options": [
        {
          "name": "TypeScript",
          "value": "typescript",
          "description": "Optimize TypeScript compilation"
        },
        {
          "name": "JavaScript",
          "value": "javascript",
          "description": "Optimize JavaScript bundling"
        },
        {
          "name": "Full Stack",
          "value": "fullstack",
          "description": "Optimize entire application build"
        }
      ],
      "default": "typescript"
    },
    {
      "displayName": "Optimization Level",
      "name": "optimizationLevel",
      "type": "options",
      "options": [
        {
          "name": "Basic",
          "value": "basic",
          "description": "Basic optimizations (1.2x speedup)"
        },
        {
          "name": "Advanced",
          "value": "advanced",
          "description": "Advanced optimizations (2x speedup)"
        },
        {
          "name": "Quantum",
          "value": "quantum",
          "description": "Quantum-enhanced optimizations (2.3x speedup)"
        }
      ],
      "default": "quantum"
    },
    {
      "displayName": "Target Directory",
      "name": "targetDirectory",
      "type": "string",
      "default": "./src",
      "description": "Directory to optimize"
    }
  ],
  "webhooks": [],
  "polling": false
}