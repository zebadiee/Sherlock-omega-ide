{
  "name": "sherlock-omega-ide",
  "displayName": "Sherlock Œ© - Zero-Friction Development",
  "description": "Revolutionary AI-powered development environment that eliminates dependency friction and maintains perfect flow state",
  "version": "1.0.0",
  "publisher": "sherlock-omega",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Linters",
    "Formatters",
    "Snippets"
  ],
  "keywords": [
    "ai",
    "dependencies",
    "zero-friction",
    "auto-install",
    "flow-state",
    "computational-consciousness",
    "sherlock-omega"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact",
    "onLanguage:json",
    "onCommand:sherlock-omega.activate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sherlock-omega.activate",
        "title": "üß† Activate Sherlock Œ©",
        "category": "Sherlock Œ©"
      },
      {
        "command": "sherlock-omega.runFrictionDetection",
        "title": "üîç Run Friction Detection",
        "category": "Sherlock Œ©"
      },
      {
        "command": "sherlock-omega.showActionPlan",
        "title": "üìã Show Action Plan",
        "category": "Sherlock Œ©"
      },
      {
        "command": "sherlock-omega.installAllDependencies",
        "title": "üì¶ Install All Missing Dependencies",
        "category": "Sherlock Œ©"
      },
      {
        "command": "sherlock-omega.toggleZeroFriction",
        "title": "‚ö° Toggle Zero-Friction Mode",
        "category": "Sherlock Œ©"
      },
      {
        "command": "sherlock-omega.showMetrics",
        "title": "üìä Show Zero-Friction Metrics",
        "category": "Sherlock Œ©"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "sherlock-omega.runFrictionDetection",
          "when": "editorLangId == typescript || editorLangId == javascript",
          "group": "sherlock-omega"
        }
      ],
      "explorer/context": [
        {
          "command": "sherlock-omega.installAllDependencies",
          "when": "resourceFilename == package.json",
          "group": "sherlock-omega"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "sherlock-omega-action-plan",
          "name": "üß† Sherlock Œ© Action Plan",
          "when": "sherlock-omega.activated"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sherlock-omega",
          "title": "Sherlock Œ©",
          "icon": "$(brain)"
        }
      ]
    },
    "configuration": {
      "title": "Sherlock Œ©",
      "properties": {
        "sherlock-omega.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable Sherlock Œ© zero-friction development"
        },
        "sherlock-omega.autoInstallDependencies": {
          "type": "boolean",
          "default": true,
          "description": "Automatically install missing dependencies"
        },
        "sherlock-omega.packageManager": {
          "type": "string",
          "enum": ["auto", "npm", "yarn", "pnpm"],
          "default": "auto",
          "description": "Preferred package manager (auto-detect if 'auto')"
        },
        "sherlock-omega.detectionDelay": {
          "type": "number",
          "default": 500,
          "description": "Delay in milliseconds before running friction detection"
        },
        "sherlock-omega.showNotifications": {
          "type": "boolean",
          "default": true,
          "description": "Show notifications for friction elimination"
        },
        "sherlock-omega.confidenceThreshold": {
          "type": "number",
          "default": 0.8,
          "minimum": 0.1,
          "maximum": 1.0,
          "description": "Minimum confidence threshold for auto-actions"
        }
      }
    },
    "languages": [
      {
        "id": "sherlock-omega-log",
        "aliases": ["Sherlock Œ© Log"],
        "extensions": [".sherlock.log"]
      }
    ],
    "grammars": [
      {
        "language": "sherlock-omega-log",
        "scopeName": "text.log.sherlock-omega",
        "path": "./syntaxes/sherlock-omega-log.tmGrammar.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.0",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "sherlock-omega-ide": "file:../",
    "vscode-languageclient": "^8.0.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zebadiee/Sherlock-omega-ide.git"
  },
  "bugs": {
    "url": "https://github.com/zebadiee/Sherlock-omega-ide/issues"
  },
  "homepage": "https://sherlock-omega.dev",
  "license": "MIT",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#667eea",
    "theme": "dark"
  }
}